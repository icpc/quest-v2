{
  "name": "icpc-quest",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "pocketbase": "^0.26.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "react-spinners": "^0.17.0",
    "styled-components": "^6.1.19"
  },
  "scripts": {
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build",
    "start": "vite",
    "build": "tsc -b && vite build",
    "pocketbase": "cd pocketbase && pocketbase serve --dev --migrationsDir=./pb_migrations",
    "pocketbase:migration": "cd pocketbase && pocketbase migrate collections --migrationsDir=./pb_migrations",
    "pocketbase:typegen": "cd pocketbase && pocketbase-typegen --db ./pb_data/data.db --out ../src/types/pocketbase-types.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/css": "^0.8.1",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.1",
    "@types/papaparse": "^5.5.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.4.1",
    "pocketbase-typegen": "^1.3.2"
  }
}